{
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.background": "#1C1E2A",
    "editor.background": "#0E0F1A",
    "editor.foreground": "#EAEAEA",
    "editor.inactiveSelectionBackground": "#2c313a4b",
    "editor.lineHighlightBackground": "#2A2B3A",
    "editor.selectionBackground": "#3E4451",
    "editorCursor.foreground": "#FFCC00",
    "editorIndentGuide.activeBackground1": "#4B4F5A",
    "editorIndentGuide.background1": "#2C2F38",
    "editorLineNumber.foreground": "#4B526D",
    "sideBar.background": "#12131A",
    "statusBar.background": "#1A1B26",
    "tab.activeBackground": "#1E1F2A",
    "tab.inactiveBackground": "#12131A",
    "terminal.background": "#0E0F1A",
    "terminal.foreground": "#C7C7C7",
    "titleBar.activeBackground": "#1C1E2A",
    "titleBar.inactiveBackground": "#1C1E2A"
  },
  "name": "Dark Angel",
  "tokenColors": [
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.class.component.html"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "meta.tag.attributes",
        "entity.other.attribute-name",
        "variable.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.css",
        "string.quoted.single.css"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "source.css support.type.property-name",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "source.css keyword.other.unit",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "support.constant.json",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.terminator.statement",
        "punctuation.definition.string",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end"
      ],
      "settings": {
        "foreground": "#EAEAEA"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6370"
      }
    },
    {
      "scope": [
        "meta.function.c",
        "meta.function.definition.c",
        "meta.function.definition.parameters.c"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "meta.function.python",
        "entity.name.function.python"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "variable.language.python",
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "punctuation.bracket",
      "settings": {
        "foreground": "#C7C7C7"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6370"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F44747"
      }
    }
  ],
  "type": "dark"
}
